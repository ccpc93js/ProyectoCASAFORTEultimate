    "dotenv": "^8.6.0",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mercadopago": "^1.5.7",
